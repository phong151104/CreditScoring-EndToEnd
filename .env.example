# Environment Variables Template
# Copy this file to .env and fill in your actual values

# ============================================
# LLM Configuration
# ============================================

# Google Gemini API Key (Recommended - Fast & Free tier available)
GOOGLE_API_KEY=
GOOGLE_MODEL=gemini-2.5-pro

# LLM Provider (google, openai, or anthropic)
LLM_PROVIDER=google

# LLM Settings
LLM_MAX_TOKENS=8000
LLM_TEMPERATURE=0.7

